#include<iostream>
using namespace std;

void printArray(int arr[], int n) {
    for(int i=0; i<n; i++) {
        cout << arr[i] <<",";
    }
    cout << endl;
}

int bubbleSort(int arr[], int n) {
    for(int i=0; i<n; i++)  {
        for(int j=0; j<n-1; j++) {
            if(arr[j] > arr[j+1]) {
                swap(arr[j], arr[j+1]);
            }
        }
    }
    printArray(arr,n);
}
int main() {
    int arr[] = {9,1,7,3,5,4,2,6,8};
    int n = sizeof(arr) / sizeof(int);

    bubbleSort(arr,n);
    return 0;
}
